<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mappings xmlns="http://dozermapper.github.io/schema/bean-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozermapper.github.io/schema/bean-mapping https://dozermapper.github.io/schema/bean-mapping.xsd">
    <configuration>
        <wildcard>false</wildcard>
    </configuration>
    <mapping>
        <class-a>forecastsource.ForecastSource</class-a>
        <class-b>forecastresponse.ForecastResponse</class-b>
        <field>
            <a>city.country</a>
            <b>City.Country</b>
        </field>
        <field>
            <a>city.coord.lon</a>
            <b>City.Longitude</b>
        </field>
        <field>
            <a>city.coord.lat</a>
            <b>City.Latitude</b>
        </field>
        <field>
            <a>city.name</a>
            <b>City.City</b>
        </field>
        <field>
            <a>city.population</a>
            <b>City.Population</b>
        </field>
        <field>
            <a>list</a>
            <b>list</b>
        </field>
    </mapping>
    <mapping>
        <class-a>forecastsource.List</class-a>
        <class-b>forecastresponse.List</class-b>
        <field>
            <a>dtTxt</a>
            <b>DateTime</b>
        </field>
        <field>
            <a>weather</a>
            <b>Weather</b>
        </field>
        <field>
            <a>main.temp</a>
            <b>Weather.Tempreture</b>
        </field>
        <field>
            <a>main.humidity</a>
            <b>Weather.Humidity</b>
        </field>
        <field>
            <a>main.grndLevel</a>
            <b>Weather.GroundLevel</b>
        </field>
        <field>
            <a>main.tempMin</a>
            <b>Weather.TempretureMin</b>
        </field>
        <field>
            <a>main.seaLevel</a>
            <b>Weather.SeaLevel</b>
        </field>
        <field>
            <a>main.pressure</a>
            <b>Weather.Pressure</b>
        </field>
        <field>
            <a>main.tempMax</a>
            <b>Weather.TempretureMax</b>
        </field>
        <field>
            <a>wind.speed</a>
            <b>Weather.WindSpeed</b>
        </field>
        <field>
            <a>wind.deg</a>
            <b>Weather.WindDegree</b>
        </field>
    </mapping>
    <mapping>
        <class-a>forecastsource.Weather</class-a>
        <class-b>forecastresponse.List</class-b>
        <field>
            <a>description</a>
            <b>Weather.Description</b>
        </field>
    </mapping>
</mappings>
