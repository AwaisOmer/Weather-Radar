<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mappings xmlns="http://dozermapper.github.io/schema/bean-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozermapper.github.io/schema/bean-mapping https://dozermapper.github.io/schema/bean-mapping.xsd">
    <configuration>
        <wildcard>false</wildcard>
    </configuration>
    <mapping>
        <class-a>currentsource.CurrentSource</class-a>
        <class-b>currentresponse.CurrentResponse</class-b>
        <field>
            <a>dt</a>
            <b>DateTime</b>
        </field>
        <field>
            <a>coord.lon</a>
            <b>City.Longitude</b>
        </field>
        <field>
            <a>coord.lat</a>
            <b>City.Latitude</b>
        </field>
        <field>
            <a>name</a>
            <b>City.City</b>
        </field>
        <field>
            <a>sys.country</a>
            <b>City.Country</b>
        </field>
        <field>
            <a>sys.sunrise</a>
            <b>City.Population</b>
        </field>
        <field>
            <a>main.tempMin</a>
            <b>Weather.TempretureMin</b>
        </field>
        <field>
            <a>main.grndLevel</a>
            <b>Weather.GroundLevel</b>
        </field>
        <field>
            <a>main.humidity</a>
            <b>Weather.Humidity</b>
        </field>
        <field>
            <a>main.temp</a>
            <b>Weather.Tempreture</b>
        </field>
        <field>
            <a>main.pressure</a>
            <b>Weather.Pressure</b>
        </field>
        <field>
            <a>main.seaLevel</a>
            <b>Weather.SeaLevel</b>
        </field>
        <field>
            <a>main.tempMax</a>
            <b>Weather.TempretureMax</b>
        </field>
        <field>
            <a>wind.deg</a>
            <b>Weather.WindDegree</b>
        </field>
        <field>
            <a>wind.speed</a>
            <b>Weather.WindSpeed</b>
        </field>
    </mapping>
    <mapping>
        <class-a>currentsource.Weather</class-a>
        <class-b>currentresponse.CurrentResponse</class-b>
        <field>
            <a>description[0]</a>
            <b>Weather.Description</b>
        </field>
    </mapping>
</mappings>
